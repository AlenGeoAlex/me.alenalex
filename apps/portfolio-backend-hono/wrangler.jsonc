{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "portfolio-backend-hono",
	"main": "src/index.ts",
	"compatibility_date": "2025-09-27",
	"observability": {
		"enabled": true
	},
	"routes": [
		{
			"pattern": "portfolio-api.alenalex.me",
			"custom_domain": true
		}
	],
	"ratelimits": [
		{
			"name": "CONTACT_ME_LIMIT",
			"namespace_id": "1000",
			"simple": {
				"limit": 10,
				"period": 60
			}
		},
		{
			"name": "API_LIMIT",
			"namespace_id": "1001",
			"simple": {
				"limit": 80,
				"period": 60
			}
		}
	],
	"send_email": [
		{
			"name": "EMAIL_BINDING",
			"destination_address": ""
		}
	]
}